#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55cb04ef0230 .scope module, "encoder_tb" "encoder_tb" 2 1;
 .timescale 0 0;
v0x55cb04f03960_0 .var "en", 0 0;
v0x55cb04f03a30_0 .var "in", 31 0;
v0x55cb04f03b00_0 .net "out", 4 0, v0x55cb04f03680_0;  1 drivers
S_0x55cb04ef03b0 .scope module, "encoder_uut" "encoder32_5" 2 7, 3 1 0, S_0x55cb04ef0230;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "en"
    .port_info 1 /INPUT 32 "A"
    .port_info 2 /OUTPUT 5 "Y"
v0x55cb04ece2a0_0 .net "A", 31 0, v0x55cb04f03a30_0;  1 drivers
v0x55cb04f03680_0 .var "Y", 4 0;
v0x55cb04f03760_0 .net "en", 0 0, v0x55cb04f03960_0;  1 drivers
v0x55cb04f03800_0 .var/i "i", 31 0;
E_0x55cb04ef0ce0 .event edge, v0x55cb04f03760_0, v0x55cb04f03800_0, v0x55cb04ece2a0_0;
    .scope S_0x55cb04ef03b0;
T_0 ;
    %wait E_0x55cb04ef0ce0;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x55cb04f03680_0, 0, 5;
    %load/vec4 v0x55cb04f03760_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55cb04f03800_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x55cb04f03800_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x55cb04ece2a0_0;
    %load/vec4 v0x55cb04f03800_0;
    %part/s 1;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x55cb04f03800_0;
    %pad/s 5;
    %store/vec4 v0x55cb04f03680_0, 0, 5;
T_0.4 ;
    %load/vec4 v0x55cb04f03800_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x55cb04f03800_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55cb04ef0230;
T_1 ;
    %vpi_call 2 10 "$monitor", "in = %b, out = %b, en = %b", v0x55cb04f03a30_0, v0x55cb04f03b00_0, v0x55cb04f03960_0 {0 0 0};
    %vpi_call 2 11 "$dumpfile", "encoder.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55cb04ef0230 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cb04f03960_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cb04f03960_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cb04f03960_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 16479, 0, 32;
    %store/vec4 v0x55cb04f03a30_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cb04f03960_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_1;
